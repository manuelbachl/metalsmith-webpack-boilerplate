{
  "name": "metalsmith-webpack-boilerplate",
  "version": "0.1.0",
  "description": "",
  "main": "",
  "scripts": {
    "build": "DEBUG=metalsmith* npm run clean && npm run build:webpack && npm run build:metalsmith",
    "build:prod": "NODE_ENV=production npm run build",
    "build:metalsmith": "babel-node src/scripts/build-metalsmith.js",
    "build:webpack": "npm run clean:webpack && webpack",
    "clean": "npm run clean:metalsmith && npm run clean:webpack",
    "clean:metalsmith": "rimraf web/*",
    "clean:webpack": "rimraf src/assets/*",
    "dev": "npm run clean && DEBUG=metalsmith* nodemon --watch src/scripts --exec babel-node src/scripts/run-dev-server.js",
    "server": "npm run build && http-server web",
    "deploy": "npm run test && npm run build:prod && gh-pages -d web",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint ./ --cache --ignore-path .gitignore",
    "lint:scss": "stylelint assets/scss/**/*.scss",
    "test": "npm run lint && npm run test:build-process",
    "test:build-process": "npm run build",
    "preversion": "npm test",
    "version": "npm run build && git add -f web",
    "postversion": "git push && git push --tags",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "precommit": "npm run lint"
  },
  "repository": {
    "url": "ssh://git@github.com:manuelbachl/metalsmith-webpack-boilerplate.git",
    "type": "git"
  },
  "author": "Manuel Bachl <m.bachl@finndrop.de>",
  "license": "MIT",
  "homepage": "https://github.com/manuelbachl",
  "dependencies": {},
